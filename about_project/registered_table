-- public.registered definition

-- Drop table

-- DROP TABLE public.registered;

CREATE TABLE public.registered (
	reg_id serial4 NOT NULL,
	"name" varchar(25) NOT NULL,
	email varchar(25) NOT NULL,
	"password" varchar NOT NULL,
	user_type varchar NOT NULL,
	CONSTRAINT registered_pkey PRIMARY KEY (reg_id)
);
